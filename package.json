{
  "name": "functionally-coding-exercise",
  "version": "1.0.0",
  "description": "Server-side Rendering React from Scratch!",
  "main": "index.js",
  "engines": {
    "node": "8.10.0",
    "npm": "5.7.1"
  },
  "author": "Tirta Nugraha <dev.nugrata@gmail.com>",
  "scripts": {
    "start": "node index.js",
    "pack": "webpack -p --config webpack.config.js",
    "babel": "babel src -d views",
    "build": "npm run pack && npm run babel",
    "clean": "rm -rf views && rm -rf build"
  },
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  }
}
